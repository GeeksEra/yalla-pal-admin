femaleRange.label.fill = chart.colors.list[1];
femaleRange.label.dy = 20;
femaleRange.label.fontWeight = '600';
femaleRange.grid.strokeOpacity = 1;
femaleRange.grid.stroke = female.stroke;

}); // end am4core.ready()
</script>





